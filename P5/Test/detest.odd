  <teiHeader>
    <fileDesc>
      <titleStmt>
        <title>Testing errors</title>
        <author>Lou Burnard</author>
        <author>Sebastian Rhatz</author>
        <author xml:id="sbauman.emt">Syd Bauman</author>
